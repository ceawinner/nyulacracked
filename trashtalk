game:GetService("UserInputService").InputBegan:Connect(function(Key,IsChat)
if IsChat then return end
if Key.KeyCode == Enum.KeyCode.H then
local sex = {
    "seeded, get out of my game", --[1] (doesnt start at 0)
    "you're my son",
    "sit down doggy 🐶🐶",
    "get good, get nyula",
    "keep hugging the ground son",
    "wake up lil boy",
    "whatchu aimin at",
    "cope",
    "get known",
    "go back to bloxburg", -- [10]
    "its okay, this game isnt for you",
    "taste the power of my gaming chair",
    "u play like ur paralyzed",
    "6head ahh 💀" -- 14
}

local imgettingmygradesup = math.random(1,14)
if imgettingmygradesup == lastnumber then -- checks for if the last chat or wtv was the same as the random
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(sex[imgettingmygradesup], "All")
else
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(sex[math.random(1,14)], "All")
end
local lastnumber = imgettingmygradesup -- sets last number
end
end)
